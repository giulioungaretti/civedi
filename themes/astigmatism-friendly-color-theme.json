  "tokenColors": [
    {
      "name": "Default text - monochrome",
      "scope": [
        "source",
           "meta.function-call.js",
           "meta.export.default.js",
        "text"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Comments - slightly muted",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#606060",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings - subtle distinction with italic",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.quoted.triple",
        "string.template",
        "string.interpolated",
        "string.regexp",
        "punctuation.definition.string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#404040",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String interpolation expressions - bold to stand out",
      "scope": [
        "meta.embedded",
        "meta.template.expression",
        "meta.interpolation",
        "punctuation.definition.template-expression",
        "punctuation.definition.interpolation",
        "punctuation.section.embedded",
        "punctuation.section.interpolation",
        "variable.other.readwrite.interpolated",
        "entity.name.variable.interpolated",
        "meta.string.interpolated",
        "variable.interpolation",
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "string.interpolated variable",
        "string.quoted.double.interpolated variable",
        "meta.interpolation.fsharp",
        "variable.other.interpolated.fsharp",
        "punctuation.section.interpolation.begin.fsharp",
        "punctuation.section.interpolation.end.fsharp",
        "source.fsharp meta.interpolation",
        "string.quoted.interpolated"
      ],
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords - monochrome",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Functions - monochrome",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Classes and types - monochrome",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Variables - monochrome",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language",
        "support.variable"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Constants and numbers - monochrome",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Operators and punctuation - monochrome",
      "scope": [
        "keyword.operator",
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor",
        "punctuation.definition"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Tags (HTML/XML) - monochrome",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Attributes - monochrome",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Invalid/Error - red for visibility",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#D32F2F"
      }
    },
    {
      "name": "Deprecated - warning color",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#E65100"
      }
    },
    {
      "name": "Markup headings - monochrome but bold",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#1A1A1A",
      }
    },
    {
      "name": "Markup bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup code/inline - monochrome",
      "scope": [
        "markup.inline.raw",
        "markup.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Markup links - monochrome with underline",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup lists - monochrome",
      "scope": [
        "markup.list",
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Markup quote - muted",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#606060",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inserted text (diff) - green",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#388E3C"
      }
    },
    {
      "name": "Deleted text (diff) - red",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#D32F2F"
      }
    },
    {
      "name": "Changed text (diff) - blue",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "Regex - subtle distinction",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#404040",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape characters in strings",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#404040",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Meta braces and brackets - monochrome",
      "scope": [
        "meta.brace",
        "punctuation.definition.block",
